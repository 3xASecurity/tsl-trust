<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="se.tillvaxtverket.tsltrust.weblogic.data.TslPolicy" table="tslpolicy">
        <id name="tslPolicyName" column="name" type="string" length="255"/>
        <property name="statesStr" column="states" type="string" length="65535"/>
        <property name="serviceTypesStr" column="serviceTypes" type="string" length="65535"/>
        <property name="statusTypesStr" column="statusTypes" type="string" length="65535"/>
        <property name="signStatusStr" column="signStatus" type="string" length="65535"/>
        <property name="expiredTslGrace" column="expiredTsl" type="integer"/>
    </class>
</hibernate-mapping>
